{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
  "skipLibCheck": true,
  "moduleResolution": "node",
  "strict": true,
  "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": ["react", "react-dom","@react-native-async-storage/async-storage"],

  "typeRoots": ["node_modules/@types"],
  
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
